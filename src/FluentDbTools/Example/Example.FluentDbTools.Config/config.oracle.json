{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    },
    "Console":
    {
      "IncludeScopes": true
    }
  },
  "database": {
    "type": "Oracle",
    "user": "dbuser",
    "password": "dbpassword",
    "adminUser": "system",
